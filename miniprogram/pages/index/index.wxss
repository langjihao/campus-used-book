.top-bg {
	position: absolute;
	top: 0rpx;
	left: 0rpx;
	width: 100%;
	height: 340rpx;
	background: #39b54a;
	background-repeat: no-repeat;
	background-size: 100%;
	border-radius: 0 0 100rpx 100rpx;
}
.banner_warp,.banner_warp .banner,.banner_warp image{height:264rpx;}
.banner_warp{margin:50rpx 50rpx 10rpx 50rpx;}
.banner_warp .banner_warp .li{border-radius: 20rpx;width: 92%;overflow: hidden}
.banner_warp .wx-swiper-dot{width: 40rpx;height: 6rpx;background: rgba(255,255,255,.2);border-radius: inherit;}   
.banner_warp .wx-swiper-dot-active{width: 40rpx;height: 6rpx;background: #fff;}
.banner_warp image{border-radius: 20rpx;}

.serachbar{
	background-color: #fff;
}
.tab{
	border-bottom: #39b54a 1px;
	position: fixed;
	z-index: 99999;
	top:0px;
}
.checkarea{
	align-items: center;
	height: 50rpx;
	display: flex;
	background-color: #fff;
	justify-content: space-around;
}
::-webkit-scrollbar {
      width: 0;
      height: 0;
      color: transparent;
}

.space {
      width: 100%;
      height: 20rpx;
      background: #eee;
}

