.bg{
	width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}
.box {
  border-radius: 32rpx;
	width: 100%;
  background: #fff;
  box-sizing: border-box;
	box-shadow: 0 0 20rpx #eee;
	padding: 30rpx 30rpx;
	margin: 30rpx 30rpx;
}
.UCenter-bg {
	width: 100%;
  display: flex;
	justify-content: center;
	box-sizing:border-box;
  padding: 40rpx;
  flex-direction: column;
	align-items: center;
	position: fixed;
	top: 200rpx;
}
